page {
  background-color: #f6f6f6;
}

/*编辑地址*/

.w-box {
  background-color: #fff;
  width: 100%;
  height: 690rpx;
}

.w-input {
  width: 700rpx;
  height: 80rpx;
  margin-right: auto;
  margin-left: auto;
  border-bottom-width: 1rpx;
  border-bottom-style: dashed;
  border-bottom-color: #aaa;
  display: flex;
}

.w-bt {
  width: 195rpx;
  height: 80rpx;
  margin-left: 10rpx;
  font-size: 30rpx;
  line-height: 80rpx;
}

.w-sr {
  flex: 1;
  height: 80rpx;
}

.w-inp {
  width: 100%;
  height: 80rpx;
  font-size: 30rpx;
  line-height: 80rpx;
  color: #333;
  border: none;
}

.w-texta {
  width: 100%;
  height: 160rpx;
  font-size: 30rpx;
  line-height: 40rpx;
  color: #333;
  padding-top: 23rpx;
}

.itext {
  height: auto;
}

/*按钮*/

.w-the {
  width: 100%;
  height: 95rpx;
  background-color: #fff;
  border-top-width: 1rpx;
  border-top-style: solid;
  border-top-color: #e0e0e0;
  position: fixed;
  bottom: 0rpx;
}

.w-new {
  font-size: 28rpx;
  line-height: 70rpx;
  text-align: center;
  color: #fff;
  width: 690rpx;
  height: 70rpx;
  margin-right: auto;
  margin-left: auto;
  -moz-border-radius: 30rpx;
  -webkit-border-radius: 30r5px;
  border-radius: 30rpx;
  background-color: #ffcd5f;
  margin-top: 8rpx;
}

.w-arrow {
  width: 15rpx;
  height: 25rpx;
  padding-top: 29rpx;
  padding-right: 10rpx;
}

.w-arrow .arrow {
  width: 100%;
  height: 100%;
  display: block;
}
